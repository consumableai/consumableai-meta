{
  "name": "consumableai-meta",
  "version": "1.0.0",
  "description": "Lightweight React AEO/GEO meta component for per-page title, description, canonical, robots, and OG/Twitter tags.",
  "keywords": [
    "react",
    "meta",
    "title",
    "description",
    "canonical",
    "robots",
    "open graph",
    "twitter",
    "AEO",
    "GEO",
    "answer engine optimization",
    "generative engine optimization",
    "consumable ai",
    "consumableai",
    "seo ranking",
    "consumableai ranking"
  ],
  "license": "MIT",
  "author": "Consumable AI <hello@consumableai.com>",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/consumableai/consumableai-meta"
  },
  "homepage": "https://www.consumableai.com"
}
